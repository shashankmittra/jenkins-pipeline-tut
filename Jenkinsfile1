#!/usr/bin/env groovy

@Library('shared-library-jenkins') _

pipeline{
         agent any
	
	environment {
		PATH= /opt/apache-maven-3.6.3/bin:$PATH
	}
	
         tools{
          maven 'Maven-3.6.3'
         }
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        functions (1,4)
                    }
                }
		
           }	       	     	         
}
