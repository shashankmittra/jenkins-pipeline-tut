@Library('shared-library-jenkins@master') _

pipeline{
         agent any

         tools{
          maven 'Maven 3.8'
         }
        
        stages{

              stage('maven build'){
                  steps{
                      script{
		    	                sh "mvn clean install"
                      	  }
               	     }  
                 }	
                 
                 stage ('Check logs') {
                    steps {
                        functions.add(1,4)
                        functions.multiply(2,4)
                    }
                }
		
           }	       	     	         
}